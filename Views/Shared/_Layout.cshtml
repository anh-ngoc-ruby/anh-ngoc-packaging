
@inject IGetListMenuUseCase getListMenuUseCase;
@inject IGetListProductCategoryUseCase getListProductCategoryUseCase;
@inject IGetCompanyInfoUseCase getCompanyInfoUseCase;
@{
    var companyInfo = await this.getCompanyInfoUseCase.Execute();
    var Menus = await this.getListMenuUseCase.Execute();
    var productCategories = await this.getListProductCategoryUseCase.Execute();
    LayoutResponseDto data = new LayoutResponseDto
            {
                CompanyInfo = companyInfo,
                Menus = Menus,
                ProductCategories = productCategories
            };
    var favicon = companyInfo.Images.Favicon;
    var description = companyInfo.Name;
    // var title = companyInfo.Name;
}
<!DOCTYPE html>
<html>
<head>
    <!-- ========== Meta Tags ========== -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="gramentheme">
    <meta name="description" content="Công Ty TNHH Ánh Ngọc là địa chỉ uy tín tại TP.HCM chuyên sản xuất bao bì HDPE, LDPE đa dạng quy cách, độ thấm nước cao cho các ngành may mặc, y tế, thực phẩm, v.v. giá tốt, giao hàng toàn quốc, tận nơi.">
    <!-- ======== Page title ============ -->
    <title>Công ty sản xuất,phân phối bao bì nhựa PE, PP, OPP, v.v. giá tốt, chất lượng tại TP.HCM</title>
    <!--<< Favcion >>-->
    <link rel="shortcut icon" href="@favicon">
    <!--<< Bootstrap min.css >>-->
    <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true">
    <!--<< All Min Css >>-->
    <link rel="stylesheet" href="~/css/all.min.css" asp-append-version="true">
    <!--<< Animate.css >>-->
    <link rel="stylesheet" href="~/css/animate.css" asp-append-version="true">
    <!--<< Magnific Popup.css >>-->
    <link rel="stylesheet" href="~/css/magnific-popup.css" asp-append-version="true">
    <!--<< MeanMenu.css >>-->
    <link rel="stylesheet" href="~/css/meanmenu.css" asp-append-version="true">
    <!--<< Swiper Bundle.css >>-->
    <link rel="stylesheet" href="~/css/swiper-bundle.min.css" asp-append-version="true">
    <!--<< Nice Select.css >>-->
    <link rel="stylesheet" href="~/css/nice-select.css" asp-append-version="true">
    <!--<< Icomoon.css >>-->
    <link rel="stylesheet" href="~/css/icomoon.css" asp-append-version="true">
    <!--<< Main.css >>-->
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true">
    <!--<< Bootstrap Icons >>-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">

</head>
<body>
    <!-- Back To Top Start -->
    <button id="back-top" class="back-to-top">
        <i class="fa-solid fa-chevron-up"></i>
    </button>

    <!-- Content -->

    @await Component.InvokeAsync("Offcanvas", data) 
    @await Component.InvokeAsync("HeaderMain", data)
    @await Component.InvokeAsync("HeaderSticky", data)
    @RenderBody()
    @await Component.InvokeAsync("Footer", data)
    <script type="importmap"></script>

  
    <script src="~/js/jquery-3.7.1.min.js"></script>
    <!--<< Viewport Js >>-->
    <script src="~/js/viewport.jquery.js"></script>
    <!--<< Bootstrap Js >>-->
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <!--<< Nice Select Js >>-->
    <script src="~/js/jquery.nice-select.min.js"></script>
    <!--<< Waypoints Js >>-->
    <script src="~/js/jquery.waypoints.js"></script>
    <!--<< Counterup Js >>-->
    <script src="~/js/jquery.counterup.min.js"></script>
    <!--<< Swiper Slider Js >>-->
    <script src="~/js/swiper-bundle.min.js"></script>
    <!--<< MeanMenu Js >>-->
    <script src="~/js/jquery.meanmenu.min.js"></script>
    <!--<< Magnific Popup Js >>-->
    <script src="~/js/jquery.magnific-popup.min.js"></script>
    <!--<< Wow Animation Js >>-->
    <script src="~/js/wow.min.js"></script>
    <!-- Gsap -->
    <script src="~/js/gsap.min.js"></script>
    <!--<< Main.js >>-->
    <script src="~/js/main.js"></script>
    <script src="~/js/api.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
